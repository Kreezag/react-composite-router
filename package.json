{
  "name": "react-composite-router",
  "version": "1.0.0",
  "description": "predictable react router",
  "main": "index.js",
  "scripts": {
    "jest": "node_modules/.bin/jest app/* --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliaksandr-master/react-composite-router.git"
  },
  "keywords": [
    "react",
    "router",
    "declarative",
    "predictable",
    "ui",
    "composition",
    "without",
    "magic"
  ],
  "author": "alxe.master@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-master/react-composite-router/issues"
  },
  "homepage": "https://github.com/aliaksandr-master/react-composite-router#readme",
  "dependencies": {
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0"
  },
  "jest": {
    "automock": false,
    "notify": true,
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache",
    "coverageDirectory": ".tmp/tests-coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "testRegex": "/__tests__/.+?\\.test\\.js$",
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false
  }
}
